# While Loop

# continue

angka = 0

print(f'Angka Sekarang --> {angka}') # Aksi 1

while  angka < 5:
    angka += 1
    print(f'Angka Sekarang --> {angka}')
    if angka == 3:
        print('nice!')
        continue # akan membuat loop kembali ke awal
    print('Whassup!') # aksi 2
    
print('end')
